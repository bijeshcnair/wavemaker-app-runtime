{
    "name": "wavemaker-lib",
    "version": "8.4",
    "authors": [],
    "dependencies": {
        "jquery": "2.2.3",
        "jquery-ui": "1.11.4",
        "angular": "1.5.8",
        "angular-animate": "1.5.8",
        "bootstrap": "3.3.6",
        "font-awesome": "4.6.3",
        "angular-sanitize": "1.5.8",
        "angular-bootstrap-colorpicker": "https://github.com/buberdds/angular-bootstrap-colorpicker.git#v3.0.25",
        "angular-route": "1.5.8",
        "angular-scenario": "1.5.8",
        "angular-ui-bootstrap": "https://github.com/angular-ui/bootstrap-bower.git#v2.2.0",
        "angular-toaster": "https://github.com/jirikavi/AngularJS-Toaster.git#2.0.0",
        "textAngular": "https://github.com/fraywing/textAngular.git#v1.5.1",
        "d3": "https://github.com/mbostock/d3.git#v3.5.17",
        "nvd3": "https://github.com/novus/nvd3.git#v1.8.4",
        "placeholders": "4.0.1",
        "ocLazyLoad": "https://github.com/ocombe/ocLazyLoad.git#1.0.9",
        "lodash": "https://github.com/lodash/lodash.git#4.16.4",
        "hammerjs": "2.0.8",
        "animate.css": "4.0.0.alpha-1",
        "fullcalendar": "3.0.1",
        "angular-ui-calendar": "1.0.1",
        "moment": "2.15.1",
        "ngCordova":"v0.1.27-alpha",
        "ng-cordova-oauth":"0.2.10",
        "angular-websocket": "v2.0.0",
        "wavicon": "https://github.com/wavemaker/wavicon.git#v1.0.5",
        "angular-ui-mask": "1.8.7"
    },
    "resolutions": {
        "d3": "v3.5.17",
        "angular": "1.5.8",
        "fullcalendar": "3.0.1"
    },
    "exportsOverride": {
        "jquery-ui": {
            "themes": "themes/base/*.css",
            "themes/images": "themes/base/images/*.*",
            "js": [
                "ui/core.js",
                "ui/widget.js",
                "ui/mouse.js",
                "ui/draggable.js",
                "ui/droppable.js",
                "ui/position.js",
                "ui/sortable.js",
                "ui/resizable.js",
                "ui/selectable.js",
                "ui/autocomplete.js",
                "ui/button.js",
                "ui/menu.js",
                "ui/selectmenu.js",
                "ui/slider.js",
                "ui/tooltip.js"
            ]
        },
        "animate.css": {
            ".": ["dist/*.css"]
        },
        "bootstrap": {
            "less": "less/",
            "css": [
                "dist/css/*.css",
                "dist/css/*.map"
            ],
            "fonts": "fonts/"
        },
        "font-awesome": {
            "css": "css/*.css",
            "fonts": "fonts/*"
        },
        "placeholders": {
            "js": "lib/*.js"
        },
        "textAngular": {
            "js": [
                "dist/textAngular-sanitize.min.js",
                "dist/textAngularSetup.js",
                "dist/textAngular.js",
                "dist/textAngular-rangy.min.js"
            ]
        },
        "angular-ui-mask": {
            "js": [
                "dist/mask.js",
                "dist/mask.min.js"
            ]
        },
        "ocLazyLoad": {
            "js": [
                "dist/modules/ocLazyLoad.core.js",
                "dist/modules/ocLazyLoad.loaders.common.js",
                "dist/modules/ocLazyLoad.loaders.core.js",
                "dist/modules/ocLazyLoad.loaders.cssLoader.js",
                "dist/modules/ocLazyLoad.loaders.jsLoader.js"
            ]
        },
        "wavicon" : {
          ".": "dist/wavicon/android"
        },
        "d3": {
            ".": ["d3.min.js"]
        },
        "nvd3": {
            ".": ["build/nv.d3.min.js", "build/nv.d3.css"]
        },
        "fullcalendar": {
            ".": ["dist/fullcalendar.min.js", "dist/fullcalendar.css"]
        },
        "angular-websocket": {
            ".": ["dist/angular-websocket.min.js"]
        },
        "ng-cordova-oauth": {
            ".": ["dist/ng-cordova-oauth.min.js"]
        }
    }
}
